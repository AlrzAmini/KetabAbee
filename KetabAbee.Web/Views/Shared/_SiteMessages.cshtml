
@{
    if (TempData["SuccessMessage"] != null)
    {
        <script>
            $(document).ready(function () {
                ShowMessage(' موفق ', '@TempData["SuccessMessage"]', 'success');
            });
        </script>
    }
    if (TempData["InfoMessage"] != null)
    {
        <script>
            $(document).ready(function () {
                ShowMessage('اطلاعیه', '@TempData["InfoMessage"]', 'info');
            });
        </script>
    }
    if (TempData["ErrorMessage"] != null)
    {
        <script>
            $(document).ready(function () {
                ShowMessage('خطا', '@TempData["ErrorMessage"]', 'error');
            });
        </script>
    }
    if (TempData["WarningMessage"] != null)
    {
        <script>
            $(document).ready(function () {
                ShowMessage('توجه', '@TempData["WarningMessage"]', 'warning');
            });
        </script>
    }


    if (TempData["SuccessSwal"] != null)
    {
        <script>
            $(document).ready(function () {
                ShowSwal(' موفق ', '@TempData["SuccessSwal"]', 'success');
            });
        </script>
    }
    if (TempData["InfoSwal"] != null)
    {
        <script>
            $(document).ready(function () {
                ShowSwal('اطلاعیه', '@TempData["InfoSwal"]', 'info');
            });
        </script>
    }
    if (TempData["ErrorSwal"] != null)
    {
        <script>
            $(document).ready(function () {
                ShowSwal('خطا', '@TempData["ErrorSwal"]', 'error');
            });
        </script>
    }
    if (TempData["WarningSwal"] != null)
    {
        <script>
            $(document).ready(function () {
                ShowSwal('توجه', '@TempData["WarningSwal"]', 'warning');
            });
        </script>
    }
}