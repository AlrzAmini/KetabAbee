
@{
    ViewData["Title"] = "سرور در دسترس نیست";
}

<main class="page-content mt-205">
    <div class="container">
        <div class="empty-box empty-box--404">
            <div class="message mb-5">
                <p class="text-muted mb-5">
                    سرور برای لحظاتی از دستری خارج شد!
                    <img src="/images/theme/search.svg" alt="500 error image">
                </p>
                <div class="form-element-row">
                    <a onclick="window.history.go(-1);" class="btn-element btn-info-element py-3">
                        <i class="fad fa-undo"></i>
                        بازگشت 
                    </a>
                </div>
            </div>
            <div class="pic">
                <img src="/images/theme/404.svg" alt="500 error">
            </div>
        </div>
    </div>
</main>

